include $(LITEOSTOPDIR)/config.mk

MODULE_NAME := $(notdir $(CURDIR))

CJSON_VERSION = cJSON-1.7.14

CJSON_SRC = $(wildcard $(CJSON_VERSION)/cJSON.c)

CJSON_INCLUDE := -I $(CJSON_VERSION)

LOCAL_SRCS := $(subst $(CURDIR)/,, $(CJSON_SRC))

LOCAL_INCLUDE := $(CJSON_INCLUDE)

LOCAL_CDEFS := -D_STDBOOL_H

LOCAL_FLAGS := $(LOCAL_CDEFS) $(LOCAL_INCLUDE) $(LITEOS_GCOV_OPTS)

include $(MODULE)
